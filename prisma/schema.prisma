// This is your Prisma schema file
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  username String
  fname    String
  lname    String
  email    String @unique
  country  String
  code     String
  phone    String @unique
  landline String @unique
  password String
}
